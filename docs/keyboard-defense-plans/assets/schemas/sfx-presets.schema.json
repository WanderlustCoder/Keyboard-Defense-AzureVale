{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SFX Presets",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "osc": {
        "type": "string",
        "enum": [
          "sine",
          "square",
          "triangle",
          "saw",
          "noise"
        ]
      },
      "attack": {
        "type": "number",
        "minimum": 0
      },
      "decay": {
        "type": "number",
        "minimum": 0
      },
      "sustain": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "release": {
        "type": "number",
        "minimum": 0
      },
      "freq": {
        "type": "number",
        "minimum": 0
      },
      "slide": {
        "type": "number"
      },
      "vibratoDepth": {
        "type": "number",
        "minimum": 0
      },
      "vibratoRate": {
        "type": "number",
        "minimum": 0
      },
      "filterCutoff": {
        "type": "number",
        "minimum": 0
      },
      "gain": {
        "type": "number",
        "minimum": 0
      },
      "duration": {
        "type": "number",
        "minimum": 0
      }
    },
    "required": [
      "osc",
      "attack",
      "decay",
      "sustain",
      "release",
      "freq",
      "gain"
    ]
  }
}
